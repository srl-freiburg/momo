<launch>
  <!-- params -->
  <param name="/simulator/scene_file" value="$(find simulator)scenarios/scene1.xml" type="string"/>
  <rosparam command="load" file="$(find momo)/config/parameters.yaml" />

  <!-- main simulator node -->
  <node name="simulator" pkg="simulator" type="simulator" output="log"/>

  <!-- tf broadcaster for aligning frames -->
  <node name="simulator_tf_broadcaster" pkg="simulator" type="dummy_transforms.py" respawn="false" output="log" />

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find simulator)/rviz/scene1.rviz"/>


  <!-- teleop node -->
  <!-- <node name="teleop" pkg="simulator" type="teleop" respawn="false" output="screen" /> -->

  <!-- metrics node -->
  <!-- <node name="metrics_logger" pkg="simulator" type="metrics_logger.py" respawn="false" output="screen" /> -->

  <!-- agent trace logging node -->
  <!-- <node name="data_logger" pkg="simulator" type="data_logger.py" respawn="false" output="screen" /> -->

  <!-- feature logging -->
  <!-- <node name="feature_extraction" pkg="feature_extraction" type="feature_extraction" respawn="false" output="screen" /> -->

  <!-- planning node -->
  <node name="irl_features" pkg="momo" type="momo_ros.py" respawn="false" output="log"/>
  <!-- <node name="irl_features" pkg="momo" type="irl_features.py" /> -->
</launch>
